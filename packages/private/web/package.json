{
  "name": "@fredrikkadolfsson/web",
  "version": "0.12.86",
  "description": "p11k's frontend web application.",
  "main": "index.js",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "next build && tsc --project tsconfig.server.json",
    "dev": "concurrently 'dotenv nodemon' 'yarn generate:graphql --watch'",
    "dev:all": "concurrently 'sh ./docker/run.sh' 'yarn generate:graphql --watch'",
    "dev:no-generate": "dotenv nodemon",
    "generate:graphql": "graphql-codegen",
    "generate:i18n": "babel -f .babelrc 'src/**/*.{js,ts,tsx}' > /dev/null",
    "start": "ts-node --project tsconfig.server.json ./src/server.ts"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@fredrikkadolfsson/ui": "^0.11.45",
    "apollo-boost": "0.4.7",
    "cookie": "0.4.0",
    "express": "4.17.1",
    "formik": "2.1.4",
    "next": "9.2.2",
    "next-i18next": "4.2.0",
    "next-images": "1.3.0",
    "next-with-apollo": "5.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "ts-node": "8.6.2",
    "yup": "0.28.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@graphql-codegen/cli": "1.12.2",
    "@graphql-codegen/introspection": "1.12.2",
    "@graphql-codegen/typescript": "1.12.2",
    "@graphql-codegen/typescript-operations": "1.12.2",
    "@graphql-codegen/typescript-react-apollo": "1.12.2",
    "@types/cookie": "0.3.3",
    "@types/node": "13.7.0",
    "@types/react": "16.9.19",
    "@types/yup": "0.26.30",
    "babel-plugin-emotion": "10.0.27",
    "babel-plugin-i18next-extract": "0.6.1",
    "concurrently": "5.1.0",
    "eslint-plugin-graphql": "3.1.1",
    "nodemon": "2.0.2"
  }
}
